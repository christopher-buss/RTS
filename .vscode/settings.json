{
    "editor.formatOnSave": true,
    "robloxLsp.completion.callParenthesess": true,
    "robloxLsp.completion.endAutocompletion": true,
    "rojo.pluginManagement": true,
    "explorer.fileNesting.enabled": true,
    "explorer.fileNesting.patterns": {
        "*.ts": "${capture}.js",
        "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
        "*.jsx": "${capture}.js",
        "*.tsx": "${capture}.ts",
        "tsconfig.json": "tsconfig.*.json",
        "package.json": "package-lock.json, yarn.lock",
        "init.lua": "*.lua",
        "init.server.lua": "*.lua",
        "init.client.lua": "*.lua"
    },
    "robloxLsp.intelliSense.searchDepth": 2,
    "robloxLsp.hint.enable": true,
    "robloxLsp.hint.variableType": false,
    "robloxLsp.hint.paramType": false,
    "robloxLsp.completion.displayContext": 1,
    "robloxLsp.typeChecking.mode": "Strict",
    "robloxLsp.typeChecking.showFullType": true,
    "rojo.releaseBranch": "7.x",
    "editor.inlineSuggest.enabled": true,
    "luau-lsp.sourcemap.rojoProjectFile": "default.project.json",
}