{
  "name": "ExampleProject",
  "tree": {
    "$className": "DataModel",
    "ReplicatedStorage": {
      "$className": "ReplicatedStorage",
      "MatterRemote": {
        "$path": "src/MatterRemote.model.json"
      },
      "Source": {
        "$path": "src/shared"
      },
      "Packages": {
        "$className": "Folder",
        "$path": "Packages"
      }
    },
    "ServerScriptService": {
      "$className": "ServerScriptService",
      "Source": {
        "$path": "src/server"
      }
    },
    "StarterPlayer": {
      "$className": "StarterPlayer",
      "StarterPlayerScripts": {
        "$className": "StarterPlayerScripts",
        "Source": {
          "$path": "src/client"
        }
      }
    },
    "Workspace": {
      "$className": "Workspace",
      "$properties": {
        "FilteringEnabled": true
      }
    },
    "SoundService": {
      "$className": "SoundService",
      "$properties": {
        "RespectFilteringEnabled": true
      }
    },
    "TestService": {
      "$className": "TestService",
      "Tests": {
        "$path": "tests"
      }
    }
  }
}